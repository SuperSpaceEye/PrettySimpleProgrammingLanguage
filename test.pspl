fn int while_loop(int n) {
    var int counter;

    while (more(n, 0)) {
        var int nn;
        asgn(nn, n);
        asgn_sub(n, 1);

        while(more(nn, 0)) {
            asgn_add(counter, 1);

            print_num(n);
            print_num(nn);
            print_num(counter);
            print_num(-1);

            asgn_sub(nn, 1);
        } end
    } end

    return(counter);
}

fn void main() {
    print_num(while_loop(100));

    return();
}