fn int fib_test(int n) {
    if (less_eq(n, 1)) {
        return(n);
    } end
    return(add(fib_test(sub(n, 1)), fib_test(sub(n, 2))));
}

fn int while_loop_test(int n) {
    var int counter;

    while (more(n, 0)) {
        var int nn;
        asgn(nn, n);
        asgn_sub(n, 1);

        while(more(nn, 0)) {
            #print_num(n);
            #print_num(nn);
            #print_num(counter);
            #print_num(-1);

            asgn_add(counter, 1);
            asgn_sub(nn, 1);
        } end
    } end

    return(counter);
}

fn int test1(int n) {
